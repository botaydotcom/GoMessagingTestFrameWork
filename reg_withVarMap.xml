	<TimesToTest>1</TimesToTest>
	<VarMap>
		<Var name="email">
			<Value>indotest16@test.com</Value>
		</Var>		
		<Var name="name">
			<Value>indotest16</Value>
		</Var>			
		<Var name="avatar">
			<Value>429933266161898</Value>
		</Var>
		<Var name="gender">
			<Value>0</Value>
		</Var>
	</VarMap>
	<MessageSequence>
		<Message type="RegAccount" fromClient="true">
			<Command>0x03</Command>
			<Data>
				<RegAccount>
					<EMail>{{.email}}</EMail>
					<Password>e10adc3949ba59abbe56e057f20f883e</Password>
				</RegAccount>				
			</Data>
		</Message>

		<Message type="NeedFinishReg" fromClient="false">
			<Command>0x04</Command>
			<Data>
				<NeedFinishReg>
					<UserId>{{.var1}}</UserId>
				</NeedFinishReg>
				
			</Data>
		</Message>		
		
		<Message type="FillFinishReg" fromClient="true">		
			<Command>0x05</Command>
			<Data>
				<FillFinishReg>
					<UserId>{{.var1}}</UserId>
					<NickName>{{.name}}</NickName>
					<Gender>{{.gender}}</Gender>
					<AvatarId>{{.avatar}}</AvatarId>
				</FillFinishReg>
			</Data>
		</Message>

		<Message type="LoginUserInfo" fromClient="false">
			<Command>0x02</Command>
			<Data>
				<LoginInfoResult>
					<MyInfo>
						<UserId>{{.var1}}</UserId>
						<Name>{{.email}}</Name>						
						<NickName>{{.name}}</NickName>
						<Gender>{{.gender}}</Gender>
						<AvatarId>{{.avatar}}</AvatarId>
